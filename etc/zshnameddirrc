hash -d cac=${XDG_CACHE_HOME:-$HOME/usr/share/cache} 
hash -d cf=${XDG_CONFIG_HOME:-$HOME/etc} 
hash -d D=${XDG_DOWNLOAD_DIR:-$HOME/usr/downloads} 
hash -d d=${XDG_DOCUMENTS_DIR:-$HOME/usr/docs} 
hash -d dt=${XDG_DATA_HOME:-$HOME/usr/share} 
hash -d h=$HOME 
hash -d m=${XDG_MUSIC_DIR:-$HOME/usr/music} 
hash -d mn=/mnt 
hash -d pp=${XDG_PICTURES_DIR:-$HOME/usr/pics} 
hash -d sc=$HOME/usr/bin 
hash -d src=$HOME/usr/git 
hash -d vv=${XDG_VIDEOS_DIR:-$HOME/Videos} 
